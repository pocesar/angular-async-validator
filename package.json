{
  "name": "angular-async-validator",
  "version": "1.0.2",
  "description": "Makes Angular models async validation a little less painful.",
  "main": "index.js",
  "scripts": {
    "test": "karma start tests/karma.conf.js",
    "compile": "tsc --noImplicitAny --module commonjs --target es5 --declaration typings/tsd.d.ts angular-async-validator.ts && umd AsyncValidator angular-async-validator.js --commonJS > angular-async-validator.umd.js",
    "travis": "karma start tests/karma.conf.js --single-run --browsers PhantomJS"
  },
  "devDependencies": {
    "coveralls": "2.x",
    "karma": "0.12.x",
    "karma-cli": "x",
    "mocha": "2.x",
    "karma-coverage": "0.x",
    "karma-mocha": "0.x",
    "karma-phantomjs-launcher": "0.x",
    "karma-sinon": "1.x",
    "sinon": "1.x",
    "umd": "3.x"
  },
  "keywords": [
    "angular",
    "validation",
    "validator",
    "asynchronous",
    "async",
    "directive",
    "module"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pocesar/angular-async-validator.git"
  },
  "dependencies": {
    "angular":">=1.3 <2"
  },
  "author": "Paulo Cesar <https://github.com/pocesar>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pocesar/angular-async-validator/issues"
  },
  "homepage": "https://github.com/pocesar/angular-async-validator"
}
